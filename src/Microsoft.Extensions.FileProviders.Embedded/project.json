{
  "version": "1.0.0-*",
  "compilationOptions": {
    "warningsAsErrors": true,
    "keyFile": "../../tools/Key.snk",
    "nowarn": [
      "CS1591"
    ],
    "xmlDoc": true
  },
  "description": "Implementation of ASP.NET 5 file provider abstractions for embedded resources.",
  "repository": {
    "type": "git",
    "url": "git://github.com/aspnet/filesystem"
  },
  "dependencies": {
    "Microsoft.Extensions.FileProviders.Sources": {
      "version": "1.0.0-*",
      "type": "build"
    },
    "Microsoft.Extensions.FileProviders.Abstractions": "1.0.0-*"
  },
  "frameworks": {
    "net451": {
      "frameworkAssemblies": {
        "System.Runtime": { "type": "build" }
      }
    },
    "netstandard1.3": {
      "dependencies": {
        "System.Runtime.Extensions": "4.1.0-*"
      },
      "imports": [
        "dotnet5.4"
      ]
    }
  }
}